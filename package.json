{
  "name": "claude-camille",
  "version": "0.4.6",
  "description": "An intelligent code compliance checker and embedding search tool for Claude Code",
  "main": "dist/index.js",
  "bin": {
    "camille": "./dist/cli.js",
    "camille-hook": "./bin/camille-hook.sh",
    "camille-memory-hook": "./bin/camille-memory-hook.sh"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "build:tsc": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "postinstall": "node scripts/postinstall.js || true"
  },
  "keywords": [
    "claude-code",
    "hooks",
    "openai",
    "code-review",
    "mcp",
    "embeddings"
  ],
  "author": "",
  "license": "Apache 2.0",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.56.0",
    "@lancedb/lancedb": "^0.21.1",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@supabase/supabase-js": "^2.52.0",
    "bonjour": "^3.5.0",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "figlet": "^1.7.0",
    "fuzzy": "^0.1.3",
    "glob": "^10.3.10",
    "inquirer": "^9.2.15",
    "inquirer-autocomplete-prompt": "^3.0.1",
    "kuzu": "^0.10.1",
    "lru-cache": "^11.1.0",
    "node-forge": "^1.3.1",
    "open": "^10.2.0",
    "openai": "^4.67.0",
    "ora": "^8.0.1",
    "p-queue": "^8.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bonjour": "^3.5.13",
    "@types/express": "^5.0.3",
    "@types/figlet": "^1.5.8",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.19.9",
    "@types/node-forge": "^1.3.13",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*",
    "scripts/**/*",
    "bin/**/*",
    "mcp-pipe-proxy.py"
  ],
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447"
